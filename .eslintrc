{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [
    "uber-es2015",
    "uber-jsx",
    "prettier",
    "prettier/react"
  ],
  "rules": {
    "consistent-return": 0,
    "max-len": [1, 110, 4, {"ignoreStrings": true}],
    "max-params": ["error", 6],
    "object-curly-spacing": 0,
    "babel/object-curly-spacing": 2
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel",
    "prettier"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}